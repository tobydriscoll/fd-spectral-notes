   "outputs": [
    {
     "data": {
      "text/plain": [
       "s |--> ((tan(1/2*g*(s - 1))^2 + 1)*g/M)"
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "diff(xi)"
   ]
  },
  {
